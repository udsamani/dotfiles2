TS_RELEASE=/usr/local/scaligent/release
CALLOSUM_TARGET=callosum/server/target
function sshneb {
  sshpass -p 'th0ughtSp0t' ssh -o StrictHostKeyChecking=no admin@"$1"
}

function ptomcat {
  sshpass -p 'th0ughtSp0t' rsync -ah --progress \
  -e 'ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null' \
  "$HOME"/thoughtspot/$CALLOSUM_TARGET/callosum-server-1.0-SNAPSHOT.war admin@"$1":$TS_RELEASE/$CALLOSUM_TARGET/
}

